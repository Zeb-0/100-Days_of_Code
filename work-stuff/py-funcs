def add(a, b):
    result = a + b
    return result
result = add(2, 3)
print(result)

print()

def mul(x, y):
    ans = x * y
    return ans
res = mul(2, 3)
print(res)

print()

def sub(a, b):
    minus = b - a
    return minus
answ = sub(2, 3)
print(answ)

print()

def rem(n):
    mod = n % 10
    return mod
remainder = rem(23)
print(remainder)
